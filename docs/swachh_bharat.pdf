<script>
const trigger = document.querySelector('.accessibility');
const popup = document.querySelector('.access-type');
const container = document.querySelector('.user_accessibility');

// Open on focus
trigger.addEventListener('focus', () => {
  trigger.classList.add('expand');
  trigger.setAttribute('aria-expanded', 'true');
});

// Close when focus leaves the entire widget
container.addEventListener('focusout', (e) => {
  if (!container.contains(e.relatedTarget)) {
    trigger.classList.remove('expand');
    trigger.setAttribute('aria-expanded', 'false');
  }
});
</script>

